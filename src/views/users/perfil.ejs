                  <input type="email" placeholder="e-mail"  name="email" required/>
                  <br>
                  <input type="number" placeholder="edad"  name="edad" />
                  <br>
            
                  <input type="date" min="2021-01-01" max="2021-05-01" name="fecha" />
                  <br>
                  
                  <input type="file" name="archivo" accept=".jpg" />
                  <br>
                   
                  <label> Debito </label>
                  <input type="radio" name="pago" value="Debito"  >  
                  <label> Credito </label>
