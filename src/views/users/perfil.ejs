                  <label> Otro </label>
                  <input type="radio" name="pago" value="Otro" /> 
                  
                  <br>
                  
                  <label> Servicio 1 </label>
                  <input type="checkbox" name="combo" id="check1" value="Primero" />
                  <label> Servicio 2 </label>
                  <input type="checkbox" name="combo" id="check2" value="Segundo" />
                  
                  
                  <br>
                  <textarea name="comentario"> </textarea>
                    <br>
                  
                    
                    <label> cantidad </label>
                  
                  <select>
                    <option>Uno</option>
                    <option selected>Dos</option>
                  </select>
                    
            
                  <br>
                  <button id="boton1" type="reset"> Reiniciar </button>
                  <button type="submit"> Enviar </button>        
                  
                </form>
            </div>
        </div>
        <div>
            <div class="logo-inferior">
            </div>
