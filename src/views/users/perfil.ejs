<body class="perfil-body">
    <div id="caja-principal">
        <div class="barra-cuenta">
            <div class="micuenta">
               <h4>MI CUENTA</h4>
            </div>
            <div class="info-cuenta">
                <h4>INFORMACION DE MI CUENTA</h4>
            </div>
        </div>
        <div class="caja-datos">
            <div class="micuenta-caja">
                <br>
                <strong>Mis datos personales</strong>
